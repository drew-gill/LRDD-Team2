"language": "GDScript",
"path": "res://Scripts/game.gd"
}, {
"base": "Node2D",
"class": "BoardTile",
"language": "GDScript",
"path": "res://Scripts/BoardTile.gd"
}, {
"base": "Node2D",
"class": "Piece",
"language": "GDScript",
"path": "res://Prefabs/Piece.gd"
} ]
_global_script_class_icons={
"Board": "",
"BoardTile": "",
"Piece": ""
}

[application]

config/name="Long Road to Drug Discovery"
run/main_scene="res://Scenes/GameBoardScrolling.tscn"
config/icon="res://icon.png"

[autoload]

GlobalVar="*res://Scripts/GlobalVar.gd"

[display]

window/size/width=1280
window/size/height=720

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
